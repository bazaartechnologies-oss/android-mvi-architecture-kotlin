<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="OtpView">
        <attr name="pinLength" format="integer" />
        <attr name="inputType" format="enum">
            <enum name="pin" value="0" />
            <enum name="otp" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="StickyScrollView">
        <attr name="stuckShadowHeight" format="dimension" />
        <attr name="stuckShadowDrawable" format="reference" />
    </declare-styleable>
    <declare-styleable name="CounterFab">
        <attr name="badgeTextColor" format="color" />
        <attr name="badgeBackgroundColor" format="color" />
        <attr name="badgePosition" format="enum">
            <enum name="RightTop" value="0" />
            <enum name="LeftBottom" value="1" />
            <enum name="LeftTop" value="2" />
            <enum name="RightBottom" value="3" />
        </attr>
    </declare-styleable>

</resources>